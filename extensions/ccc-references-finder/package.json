{
    "package_version": 2,
    "version": "1.0.0",
    "name": "ccc-references-finder",
    "description": "i18n:ccc-references-finder.description",
    "main": "./dist/main.js",
    "devDependencies": {
        "@types/node": "^16.0.1",
        "typescript": "^4.3.4"
    },
    "author": "zhulc",
    "editor": ">=3.4.2",
    "scripts": {
        "build": "tsc -b",
        "watch": "tsc -w"
    },
    "contributions": {
        "assets": {
            "menu": {
                "methods": "./dist/assets-menu.js",
                "assetMenu": "onAssetMenu"
            }
        },
        "messages": {
            "find_references": {
                "methods": [
                    "find_references"
                ]
            },
            "find_references_in_node": {
                "methods": [
                    "find_references_in_node"
                ]
            },
            "asset-db:asset-change": {
                "methods": [
                    "assetdb_assetchange"
                ]
            },
            "replaceuuids": {
                "methods": [
                    "replaceuuids"
                ]
            }
        },
        "shortcuts": [
            {
                "message": "find_references",
                "win": "F6"
            },
            {
                "message": "find_references_in_node",
                "win": "F7"
            }
        ]
    }
}
